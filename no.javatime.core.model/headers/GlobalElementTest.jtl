import no.javatime.core.model.elements.State
import no.javatime.core.model.annotations.Start
import no.javatime.core.model.annotations.Action
import javax.inject.Inject;

package no.javatime.sample.xtext.bundle {
	
	element GlobalElementTest extends State {
		
		op @Start initialize () : Double {
		
			return 0d
		}
		
		op @Action test () : Double { 
			return value + initialize*time.dt;
		}		
	} 
}